@article{yangArbitrarilyHighOrderConservative2022,
  title = {Arbitrarily {{High-Order Conservative Schemes}} for the {{Generalized Korteweg--de Vries Equation}}},
  author = {Yang, Kai},
  year = {2022},
  month = aug,
  journal = {Siam J. Sci. Comput.},
  series = {{{EI}}},
