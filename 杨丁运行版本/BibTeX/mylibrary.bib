@article{liuMassEnergyConservation,
  title = {A {{Mass}} and {{Energy Conservation Algorithm}} for the {{Two-dimensional Fractional Nonlinear Schr{\"o}dinger Equation With Wave Operator}}},
  author = {Liu, Yang and Ran, Maohua and Zhang, Li},
  pages = {25},
  abstract = {The focus of this paper is to construct structure-preserving numerical algorithms for the fractional nonlinear Schr{\"o}dinger equation with wave operator in two dimensions. The main contribution is that the proposed Fourier pseudo-spectral method preserve both mass and energy, which is based on the newly developed partitioned averaged vector field plus method. To this end, the model problem is first reformulated as an infinite dimensional canonical Hamiltonian system by virtue of the variational derivative of the functional with fractional Laplacian. A finite dimensional canonical Hamiltonian system is then established, which is equivalent to the resulting semi-discrete conservative system by using the Fourier pseudo-spectral method in space to discrete the infinite dimensional canonical Hamiltonian system. A fully-discrete scheme is then proposed by applying the partitioned averaged vector field plus method to the semi-discrete system. The obtained fully-discrete scheme is proven to preserve both original mass and energy in discrete level. For comparison, more numerical algorithms are also listed. Finally, several numerical experiments are given to support our theoretical analysis results.},
  langid = {english},
  file = {/Users/turingscat/Zotero/storage/G6ITUHXD/A Mass and Energy Conservation Algorithm for the Two-dimensional Fractional Nonlinear SchrÃ¶dinger Equation With Wave Operator.pdf}
}

