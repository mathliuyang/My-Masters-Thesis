@article{nakajimaNeuralSchrodingerEquation2022,
  title = {Neural {{Schr{\"o}dinger Equation}}: {{Physical Law}} as {{Deep Neural Network}}},
  shorttitle = {Neural {{Schr{\"o}dinger Equation}}},
  author = {Nakajima, Mitsumasa and Tanaka, Kenji and Hashimoto, Toshikazu},
  year = {2022},
  month = jun,
  journal = {IEEE T Neur Net Lear},
  series = {{{EI}}},
  volume = {33},
  number = {6},
  pages = {2686--2700},
  issn = {2162-2388},
  doi = {10.1109/TNNLS.2021.3120472},
  abstract = {We show a new family of neural networks based on the Schr{\"o}dinger equation (SE-NET). In this analogy, the trainable weights of the neural networks correspond to the physical quantities of the Schr{\"o}dinger equation. These physical quantities can be trained using the complex-valued adjoint method. Since the propagation of the SE-NET can be described by the evolution of physical systems, its outputs can be computed by using a physical solver. The trained network is transferable to actual optical systems. As a demonstration, we implemented the SE-NET with the Crank{\textendash}Nicolson finite difference method on Pytorch. From the results of numerical simulations, we found that the performance of the SE-NET becomes better when the SE-NET becomes wider and deeper. However, the training of the SE-NET was unstable due to gradient explosions when SE-NET becomes deeper. Therefore, we also introduced phase-only training, which only updates the phase of the potential field (refractive index) in the Schr{\"o}dinger equation. This enables stable training even for the deep SE-NET model because the unitarity of the system is kept under the training. In addition, the SE-NET enables a joint optimization of physical structures and digital neural networks. As a demonstration, we performed a numerical demonstration of end-to-end machine learning (ML) with an optical frontend toward a compact spectrometer. Our results extend the application field of ML to hybrid physical-digital optimizations.},
  copyright = {10.746},
  keywords = {Integrated optics,machine learning (ML),Mathematical models,neural network,Neural networks,Optical imaging,Optical network units,Optical sensors,Optical waveguides,reservoir computing (RC),Training,waveguide},
  annotation = {sciif: 14.26 sci: Q1},
  file = {/Users/turingscat/Zotero/storage/P2LEWRIH/2022_Neural SchrÃ¶dinger Equation.pdf}
}

