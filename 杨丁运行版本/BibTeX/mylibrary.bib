@article{guoMassEnergyMomentumPreserving2023,
  title = {Mass-, {{Energy-}}, and {{Momentum-Preserving Spectral Scheme}} for {{Klein-Gordon-Schr{\"o}dinger System}} on {{Infinite Domains}}},
  author = {Guo, Shimin and Mei, Liquan and Yan, Wenjing and Li, Ying},
