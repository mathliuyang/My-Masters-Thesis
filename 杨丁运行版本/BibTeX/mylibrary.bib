@article{juStabilizedExponentialSAVSchemes2022,
  title = {Stabilized {{Exponential-SAV Schemes Preserving Energy Dissipation Law}} and {{Maximum Bound Principle}} for {{The Allen}}{\textendash}{{Cahn Type Equations}}},
  author = {Ju, Lili and Li, Xiao and Qiao, Zhonghua},
  year = {2022},
  month = jul,
  journal = {J Sci Comput},
  volume = {92},
  number = {2},
  pages = {66},
  issn = {1573-7691},
  doi = {10.1007/s10915-022-01921-9},
  url = {https://doi.org/10.1007/s10915-022-01921-9},
  urldate = {2023-06-17},
  abstract = {It is well-known that the Allen{\textendash}Cahn equation not only satisfies the energy dissipation law but also possesses the maximum bound principle (MBP) in the sense that the absolute value of its solution is pointwise bounded for all time by some specific constant under appropriate initial/boundary conditions. In recent years, the scalar auxiliary variable (SAV) method and many of its variants have attracted much attention in numerical solutions for gradient flow problems due to their inherent advantage of preserving certain discrete analogues of the energy dissipation law. However, existing SAV schemes usually fail to preserve the MBP when applied to the Allen{\textendash}Cahn equation. In this paper, we develop and analyze new first- and second-order stabilized exponential-SAV schemes for a class of Allen{\textendash}Cahn type equations, which are shown to simultaneously preserve the energy dissipation law and MBP in discrete settings. In addition, optimal error estimates for the numerical solutions are rigorously obtained for both schemes. Extensive numerical tests and comparisons are also conducted to demonstrate the performance of the proposed schemes.},
  langid = {english},
  keywords = {35K55,35K57,65M12,65M15,Energy dissipation,Exponential scalar auxiliary variable,Maximum bound principle,Stabilized method},
  file = {/Users/turingscat/Zotero/storage/E7N87B4A/2022_J Sci Comput_Stabilized Exponential-SAV Schemes Preserving Energy Dissipation Law and Maximum Bound Principle for The Allenâ€“Cahn Type Equations.pdf}
}

@article{kaniDRRNNDeepResidual2017,
