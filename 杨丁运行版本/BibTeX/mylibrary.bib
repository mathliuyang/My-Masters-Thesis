@article{colinSemidiscretizationTimeNonlinear,
  title = {Semi-Discretization in Time for {{Nonlinear Zakharov Waves Equations}}.},
  author = {Colin, T and Ebrard, G and Gallice, G},
  abstract = {In this paper we construct and study discretizations of a nonlinear Zakharov-wave system occurring in plasma physics. These systems are generalizations of the Zakharov system that can be recovered by taking a singular limit. We introduce two numerical schemes that take into account this singular limit process. One of the scheme is conservative but sensible to the polarization of the initial data while the other one is able to handle ill-prepared initial data. We prove some convergence results and we perform some numerical tests.},
  langid = {english},
  file = {/Users/turingscat/Zotero/storage/9WAQIPJ2/Colin ç­‰ - Semi-discretization in time for Nonlinear Zakharov.pdf}
}

