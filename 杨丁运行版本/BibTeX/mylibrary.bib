@article{yangConvergenceAnalysisInvariant2020,
  title = {Convergence {{Analysis}} for the {{Invariant Energy Quadratization}} ({{IEQ}}) {{Schemes}} for {{Solving}} the {{Cahn}}{\textendash}{{Hilliard}} and {{Allen}}{\textendash}{{Cahn Equations}} with {{General Nonlinear Potential}}},
