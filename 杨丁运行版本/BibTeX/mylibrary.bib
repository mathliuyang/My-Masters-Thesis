@article{huConvergenceStructurePreserving2021,
  title = {On Convergence of a Structure Preserving Difference Scheme for Two-Dimensional Space-Fractional Nonlinear {{Schr{\"o}dinger}} Equation and Its Fast Implementation},
  shorttitle = {ğŸ”¤},
  author = {Hu, Dongdong and Gong, Yuezheng and Wang, Yushun},
  year = {2021},
  month = sep,
  journal = {Comput Math Appl},
  series = {{{EI}}},
  volume = {98},
