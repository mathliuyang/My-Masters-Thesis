@article{yangNumericalMethodsFractional2010,
  title = {Numerical Methods for Fractional Partial Differential Equations with {{Riesz}} Space Fractional Derivatives},
  author = {Yang, Q. and Liu, F. and Turner, I.},
  year = {2010},
  month = jan,
  journal = {Appl Math Model},
  series = {{{EI}}},
  volume = {34},
  number = {1},
  pages = {200--218},
  issn = {0307-904X},
  doi = {10.1016/j.apm.2009.04.006},
  url = {https://www.sciencedirect.com/science/article/pii/S0307904X09001127},
  urldate = {2022-11-07},
  abstract = {In this paper, we consider the numerical solution of a fractional partial differential equation with Riesz space fractional derivatives (FPDE-RSFD) on a finite domain. Two types of FPDE-RSFD are considered: the Riesz fractional diffusion equation (RFDE) and the Riesz fractional advection{\textendash}dispersion equation (RFADE). The RFDE is obtained from the standard diffusion equation by replacing the second-order space derivative with the Riesz fractional derivative of order {$\alpha\in$}(1,2]. The RFADE is obtained from the standard advection{\textendash}dispersion equation by replacing the first-order and second-order space derivatives with the Riesz fractional derivatives of order {$\beta\in$}(0,1) and of order {$\alpha\in$}(1,2], respectively. Firstly, analytic solutions of both the RFDE and RFADE are derived. Secondly, three numerical methods are provided to deal with the Riesz space fractional derivatives, namely, the L1/L2-approximation method, the standard/shifted Gr{\"u}nwald method, and the matrix transform method (MTM). Thirdly, the RFDE and RFADE are transformed into a system of ordinary differential equations, which is then solved by the method of lines. Finally, numerical results are given, which demonstrate the effectiveness and convergence of the three numerical methods.},
  copyright = {4.162},
  langid = {english},
  keywords = {Fractional advection{\textendash}dispersion equation,L1/L2-approximation method,Matrix transform method,Method of lines,notion,Riesz space fractional derivative,Standard/shifted Gr{\"u}nwald method},
  annotation = {sciif: 5.34 sci: Q1},
  file = {/Users/turingscat/Zotero/storage/EPUDPYXM/2010_Applied Mathematical Modelling_Numerical methods for fractional partial differential equations with Riesz space fractional derivatives.pdf}
}

