@article{cuomoScientificMachineLearning2022,
  title = {Scientific {{Machine Learning Through Physics}}{\textendash}{{Informed Neural Networks}}: {{Where}} We Are and {{What}}'s {{Next}}},
  shorttitle = {🔤通过基于物理的神经网络进行科学机器学习：我们在哪里以及下一步是什么},
  author = {Cuomo, Salvatore and Cola, Vincenzo Schiano Di and Giampaolo, Fabio and Rozza, Gianluigi and Raissi, Maziar and Piccialli, Francesco},
  year = {2022},
