@article{baoComparisonsSineGordonPerturbed2010,
  title = {Comparisons between Sine-{{Gordon}} and Perturbed Nonlinear {{Schr{\"o}dinger}} Equations for Modeling Light Bullets beyond Critical Collapse},
  shorttitle = {🔤正弦 - 戈登和扰动的非线性schr{\"o}dinger方程之间的比较，用于建模临界崩溃之外的轻子弹},
  author = {Bao, Weizhu and Dong, Xuanchun and Xin, Jack},
