@article{ainsworthAnalysisApproximationFractional2017,
  title = {Analysis and {{Approximation}} of a {{Fractional Cahn--Hilliard Equation}}},
  shorttitle = {🔤{{分数Cahn--Hilliard方程的分析和近似}}},
  author = {Ainsworth, Mark and Mao, Zhiping},
  year = {2017},
  month = jan,
  journal = {Siam J. Numer. Anal.},
  series = {{{EI}}},
  volume = {55},
  number = {4},
  pages = {1689--1718},
  issn = {0036-1429, 1095-7170},
  doi = {10.1137/16M1075302},
  url = {https://epubs.siam.org/doi/10.1137/16M1075302},
  urldate = {2022-04-04},
  abstract = {🔤我们通过考虑负顺序中的梯度流sobolev空间h -{$\alpha$}，{$\alpha\in$}[0，1]来得出分数Cahn {\textendash} Hilliard方程（FCHE），其中选择{$\alpha$}= 1对应于经典的Cahn {\textendash} Hilliard方程而选择{$\alpha$}= 0恢复了Allen -CaHn方程。建立了独特的解决方案的存在，并表明该方程可保留分数阶{$\alpha$}的所有正值的质量，并且确实减少了自由能。然后，我们转向溶液的L{$\infty$}有界性的精致问题，并在非线性是四分之一的多项式的情况下为FCHE建立了fCHE的L{$\infty$}。由于估计值，我们能够证明在半分化近似方案的情况下，傅立叶 - 盖尔金方法为FCHE提供了收敛的光谱速率。最后，我们提出了使用FCHE的计算模拟获得的结果，以选择各种分数级{$\alpha$}。可以观察到，具有一般{$\alpha>$} 0的FCHE解的本质在定性上（和定量）更接近经典Cahn -Hilliard方程的行为，而不是接近Allen -Cahn方程{$\alpha$}的值是。对FCHE的粗化速率的检查表明，渐近速率对{$\alpha$}的值不敏感，因此，对于经典的Cahn-Hilliard方程式观察到的公认速率。 We derive a fractional Cahn{\textendash}Hilliard equation (FCHE) by considering a gradient flow in the negative order Sobolev space H-{$\alpha$}, {$\alpha$} {$\in$} [0, 1], where the choice {$\alpha$} = 1 corresponds to the classical Cahn{\textendash}Hilliard equation while the choice {$\alpha$} = 0 recovers the Allen{\textendash}Cahn equation. The existence of a unique solution is established and it is shown that the equation preserves mass for all positive values of fractional order {$\alpha$} and that it indeed reduces the free energy. We then turn to the delicate question of the L{$\infty$} boundedness of the solution and establish an L{$\infty$} bound for the FCHE in the case where the nonlinearity is a quartic polynomial. As a consequence of the estimates, we are able to show that the Fourier{\textendash}Galerkin method delivers a spectral rate of convergence for the FCHE in the case of a semidiscrete approximation scheme. Finally, we present results obtained using computational simulation of the FCHE for a variety of choices of fractional order {$\alpha$}. It is observed that the nature of the solution of the FCHE with a general {$\alpha$} {$>$} 0 is qualitatively (and quantitatively) closer to the behavior of the classical Cahn{\textendash}Hilliard equation than to the Allen{\textendash}Cahn equation, regardless of how close to zero the value of {$\alpha$} is. An examination of the coarsening rates of the FCHE reveals that the asymptotic rate is rather insensitive to the value of {$\alpha$} and, as a consequence, is close to the well-established rate observed for the classical Cahn{\textendash}Hilliard equation.},
  copyright = {3.508},
  langid = {american},
  keywords = {Cahn{\textendash}Hilliard equation,Fractional order,notion,Spectral method},
  annotation = {sciif: 3.04 sci: Q1 JCR分区: Q1 中科院分区升级版: 数学2区 影响因子: 2.9 5年影响因子: 3.6 EI: 是 南农高质量: A},
  file = {/Users/turingscat/Zotero/storage/GDTV3SGQ/2017_Siam J Numer Anal_Analysis and Approximation of a Fractional Cahn--Hilliard Equation.pdf}
}

@article{ainsworthWellposednessCahnHilliard2017,
  title = {Well-Posedness of the {{Cahn}}{\textendash}{{Hilliard}} Equation with Fractional Free Energy and Its {{Fourier Galerkin}} Approximation},
  shorttitle = {🔤{{带有分数自由能的Cahn}} {\textendash} {{Hilliard方程及其傅立叶盖尔金近似}}},
  author = {Ainsworth, Mark and Mao, Zhiping},
  year = {2017},
  month = sep,
  journal = {Chaos, Solitons \& Fractals},
  series = {{{EI}}},
  volume = {102},
  pages = {264--273},
  issn = {09600779},
  doi = {10.1016/j.chaos.2017.05.022},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0960077917302114},
  urldate = {2022-04-04},
  abstract = {🔤我们研究了CAHN-HILLIARD方程的适当性，其中自由能中的梯度项被分数衍生物取代。我们首先确定傅立叶 - 加盖金近似的存在和唯一性，然后对傅立叶 - 盖尔金方案得出许多先验估计。然后使用紧凑性论证来推断解决方案的解决方案的存在和独特性。然后，获得了傅立叶 - 盖尔金近似收敛速率的估计。最后，我们提出了分数cahn {\textendash} hilliard方程的典型溶液的一些数值插图，以及它们如何随分数阶{$\beta$}和参数{$\epsilon$}的变化而变化。特别是，我们展示了弥散界面的宽度{$\tau$}如何依赖于{$\epsilon$}和{$\beta$}，并得出缩放定律{$\tau$}= o（{$\epsilon$}1/{$\beta$}），该法律经过数值验证。We study the well-posedness of the Cahn{\textendash}Hilliard equation in which the gradient term in the free energy is replaced by a fractional derivative. We begin by establishing the existence and uniqueness of a Fourier-Galerkin approximation and then derive a number of priori estimates for the Fourier-Galerkin scheme. Compactness arguments are then used to deduce the existence and uniqueness of the solution to the fractional Cahn{\textendash}Hilliard equation. An estimate for the rate of convergence of the Fourier-Galerkin approximation is then obtained. Finally, we present some numerical illustrations of typical solutions to the fractional Cahn{\textendash}Hilliard equation and how they vary with the fractional order {$\beta$} and the parameter {$\epsilon$}. In particular, we show how the width {$\tau$} of the diffuse interface depends on {$\epsilon$} and {$\beta$}, and derive the scaling law {$\tau$} = O({$\epsilon$}1/{$\beta$} ) which is verified numerically.},
  copyright = {4.415},
  langid = {american},
  keywords = {Cahn{\textendash}Hilliard equation,Fractional order,notion,Spectral method},
  annotation = {28 citations (Crossref) [2022-12-02]},
  file = {/Users/turingscat/Zotero/storage/JXYDDVBY/2017_Chaos, Solitons & Fractals_Well-posedness of the Cahn–Hilliard equation with fractional free energy and its Fourier Galerkin approximation.pdf;/Users/turingscat/Zotero/storage/VILEF6P4/2017_Chaos, Solitons & Fractals_Well-posedness of the Cahn–Hilliard equation with fractional free energy and its Fourier Galerkin approximation.pdf}
}

