@phdthesis{WuJiYuDuoYueShuDuoMuBiaoDeLuYouLuXianGuiHuaYanJiuYuShiXian2021,
  type = {{硕士}},
  title = {{基于多约束多目标的旅游路线规划研究与实现}},
  author = {武, 海涛},
  year = {2021},
  doi = {10.27660/d.cnki.gzczu.2021.001901},
  url = {https://kns.cnki.net/kcms2/article/abstract?v=WGc0za3-fmKNQlFWiqcUky0OoWdP6tqs0PWoMCE2EKr-5gA9PZnk7344TVGzIfIAVotqKVLp3fs-A3lb-JhTFvGxvczD6oO4I4Zq7RmyckCPUBFMsZclL3DD8NO4aLwK&uniplatform=NZKPT&language=CHS},
  urldate = {2023-09-05},
  abstract = {随着经济的高速发展,人民生活水平明显提高,越来越多的人开始注重丰富的精神生活,促进了旅游业的发展。当前,人们主要通过网络搜寻旅游信息来定制旅游计划,但是互联网上面临的信息过载问题日益严重,使得游客在搜寻符合自己要求的旅游路线时既费时又费力。因此,越来越多的学者开始关注旅游路线规划的研究。大部分游客在制定旅游计划时都会受到交通、费用、时间、景点、酒店等多个因素的制约,然而现有研究却对游客的偏好及约束条件的考虑不够充分,很难真正满足游客的个性化需求。游客在制定自己的旅游计划时通常会遇到以下五个问题:(1)游玩哪些景点;(2)每个景点的游玩时间;(3)怎么安排自己的住宿;(4)怎么安排景点游玩的先后顺序,即怎么规划旅游路线;(5)怎么以最小的成本获取最大的心理满足感。因此,设计旅游路线时需要考虑游客的个人喜好、景点的适宜游玩时长、景点间的距离、酒店情况、交通情况,金钱花费,时间限制等多方面因素。此外,人们更加愿意接受符合自身偏好的路线信息的推荐,而不是在冗余的环境中去搜寻旅游信息。虽然当前一些旅游网站已经具备了旅游路线推荐的功能,例如途牛旅游网、去哪儿旅行、携程旅游网等,但这些网站推荐的旅游路线大多仅是一些热门景点的组合,一定程度上可以解决游客的某些需要,但不能为游客提供精准的路线规划服务,游客的旅游出行依然面临许多问题。因此,需要一个更加有效的旅游路线智能规划系统来解决这一问题。基于以上分析,本文提出一种酒店景点评分模型,基于酒店和景点的各项特性以及用户偏好约束,采用熵值法计算各约束条件的权重,来为酒店和景点进行综合打分,将得分最高的前s个酒店和前t个景点推荐给游客。然后使用推荐的酒店和景点,构建一个多约束多目标的旅游路线规划模型,并使用改进贪婪算法求解,以游客选择的酒店作为旅游路线的起点和终点,在满足游客约束的条件下为游客规划旅游路线,并将改进贪婪算法与动态规划算法比较,得出改进贪婪算法的非劣解集包含最优解的结论,且所用时间更短。最后,以武汉市为目标城市,爬取驴评网与去哪儿网的酒店和景点真实信息作为实验数据,以微博用户的旅游求助帖为例对模型进行验证,可以得到多条候选路线,并考虑景点类型多样性因素对路线重新打分和排序,得到符合该微博用户偏好的路线规划结果,说明模型具有一定的可行性与有效性。结合以上研究工作,本文基于武汉市各大旅游景点,使用Java语言的SSM框架,设计并实现了一个旅游路线智能规划系统,能够为游客提供酒店景点信息介绍、酒店景点推荐、旅游路线规划、路线行程管理等多种服务。游客登录系统以后,可以通过输入自己的出行信息来与系统交互,得到满足自身需要的个性化旅游路线,还可以对系统规划的路线进行管理,最终路线规划的结果可以以静态列表和动态地图两种方式展示给游客。},
  langid = {chinese},
  school = {中南财经政法大学},
  keywords = {Hotel attractions recommendation,Improved greedy algorithm,Multi-objective optimization,Tourism route planning,多目标优化,改进贪婪算法,旅游路线规划,酒店景点推荐},
  file = {/Users/turingscat/Zotero/storage/GBNHQLJN/武 - 2021 - 基于多约束多目标的旅游路线规划研究与实现}
}

