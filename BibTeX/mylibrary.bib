@misc{ayiStructurepreservingNumericalMethod2022,
  title = {On a Structure-Preserving Numerical Method for Fractional {{Fokker-Planck}} Equations},
  author = {Ayi, Nathalie and Herda, Maxime and Hivert, H{\'e}l{\`e}ne and Tristani, Isabelle},
  year = {2022},
  month = jul,
  number = {arXiv:2107.13416},
  eprint = {2107.13416},
  primaryclass = {cs, math},
  publisher = {{arXiv}},
  url = {http://arxiv.org/abs/2107.13416},
  urldate = {2022-11-01},
  abstract = {In this paper, we introduce and analyse numerical schemes for the homogeneous and the kinetic L{\'e}vy-Fokker-Planck equation. The discretizations are designed to preserve the main features of the continuous model such as conservation of mass, heavy-tailed equilibrium and (hypo)coercivity properties. We perform a thorough analysis of the numerical scheme and show exponential stability and convergence of the scheme. Along the way, we introduce new tools of discrete functional analysis, such as discrete nonlocal Poincar{\'e} and interpolation inequalities adapted to fractional diffusion. Our theoretical findings are illustrated and complemented with numerical simulations.},
  archiveprefix = {arxiv},
