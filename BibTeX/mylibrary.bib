  keywords = {Approximation beyond critical collapse,Efficient numerical methods,Error bounds,Light bullets,notion,Perturbed NLS,Schr{\"o}dinger equation,Sine-Gordon equation},
  annotation = {sciif: 3.75 sci: Q1 JCR分区: Q1 中科院分区升级版: 数学3区 影响因子: 4.0 5年影响因子: 3.1 EI: 是},
  file = {/Users/dasha/obsidian/Comparisons between sine-Gordon and perturbed nonlinear Schrödinger equations for modeling light bullets beyond critical collapse.md;/Users/turingscat/Zotero/storage/9KPXS89Q/bao2010.pdf.pdf;/Users/turingscat/Zotero/storage/ZNRLD3GI/2010_Physica D Nonlinear Phenomena_Comparisons between sine-Gordon and perturbed nonlinear Schrödinger equations for modeling light bullets beyond critical collapse.pdf}
